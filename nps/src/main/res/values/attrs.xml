<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NPSRecyclerView">
        <attr name="android:id" format="string"/>
        <attr name="android:orientation" format="enum"/>
        <attr name="count" format="integer"/>
        <attr name="shape" format="enum">
            <enum name="square" value="0"/>
            <enum name="circle" value="1"/>
        </attr>
    </declare-styleable>
</resources>